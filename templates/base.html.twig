    <div class="col-10 mx-auto p-4 card flex-row justify-content-center">    
        <a class="btn btn-light m-2" href="{{path('facture_index')}}">Liste facture</a>
        <a class="btn btn-light m-2" href="{{ path('facture_new') }}">Nouvelle facture</a>
        <a class="btn btn-light m-2" href="{{path('client_index')}}">Liste client</a>
        <a class="btn btn-light m-2" href="{{ path('client_new') }}">Ajouter un client</a>
        <a class="btn btn-dark m-2" href="{{ path('prestataire_index') }}">Pr√©stataire</a>
    </div>


    {% block body %}{% endblock %}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js"
